<!doctype html>

<html lang="en">
<%- include('./partials/head.ejs', { title: "Home" }) %> 
<body>
  <!-- your content here... -->
<a href="/userDetail/<%= result.id %>" text-align: left>Profile</a>
<a href="/login" text-align: right>Login</a>
  <table>
    <thead>
        <th>Name</th>
        <th>Price</th>
        <th>Stock</th>
        <th>Buy</th>

    </thead>
    <tbody>
        <% result.forEach((el) =>{%>
            <tr class="item-list">
                    <td><%= el.name %></td>
                    <td><%= el.price %> </td>
                    <td><%= el.Stock %> </td>
                    <td><form action="/login" method="get">
                        <input type="number" name="quantity">
                        <input type="submit" value="Buy">
                    </form></td>
                
            </tr>
        <% }) %> 

    </tbody>
</table>
  <script type="text/javascript" src="js/setup.js"></script>
</body>
</html>