<!doctype html>

<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="css/styles.css">

  <title>Hacktiv In Game Shop</title>
    <h1>Hacktiv In Game Shop</h1>


</head>

<body>
  <!-- your content here... -->

  <table border="2">
    <thead>
        <th>Name</th>
        <th>Price</th>
        <th>Stock</th>
        <th>Action</th>

    </thead>
    <tbody>
        <% result.forEach((el) =>{%>
            <tr class="item-list">
                    <td><%= el.name %></td>
                    <td><%= el.price %> </td>
                    <td><%= el.stock %> </td>
                    <td><a href=""><button>Edit stock</button></a> </td>
                
            </tr>
        <% }) %> 

    </tbody>
</table>
  <script type="text/javascript" src="js/setup.js"></script>
</body>
</html>